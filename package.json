{
  "name": "react-room",
  "version": "0.0.1",
  "scripts": {
    "server": "node server/server.js",
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --color --config webpack.config.js",
    "docs": "rimraf docs && cross-env NODE_ENV=development webpack --config docs.config.js",
    "build": "electron-builder build"
  },
  "main": "./dist/main.bundle.js",
  "build": {
    "appId": "com.example.app",
    "asar": true,
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.8",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.3",
    "electron": "^11.3.0",
    "electron-builder": "^22.10.5",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^4.5.2",
    "https": "^1.0.0",
    "lodash": "^4.17.21",
    "node": "^15.10.0",
    "path": "^0.12.7",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "uuid": "^8.3.2",
    "webpack": "^5.26.2",
    "webpack-cli": "^4.5.0",
    "ws": "^7.4.4"
  },
  "dependencies": {
    "@react-three/cannon": "^0.6.1",
    "@react-three/drei": "^3.11.0",
    "bootstrap": "^4.6.0",
    "browser-window": "^0.4.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.1",
    "react-three-fiber": "^5.3.19",
    "remote": "^0.2.6",
    "three": "^0.125.2",
    "use-cannon": "^0.5.3"
  }
}
